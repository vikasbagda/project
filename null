   <script> 
  function logout(url)
  {
  
    url = url+'/LogoutAction.do?userId=tct86000249';
     //Do the Ajax call
    if (window.XMLHttpRequest){ // Non-IE browsers
      req = new XMLHttpRequest();      
      //A call-back function is define so the browser knows which function to call after the server gives a reponse back
      req.onreadystatechange = populateCityBox;      
      try 
      {         
            req.open("GET", url+"&tempTime="+(new Date()), true); //was get         
      }
      catch (e) 
      { 
      }
      req.send(null);      
    } else if (window.ActiveXObject) 
      { // IE      logout
      req = new ActiveXObject("Microsoft.XMLHTTP");
      if (req) {
      req.onreadystatechange = populateCityBox;     
        req.open("GET", url+"&tempTime="+(new Date()), true);
        req.send();
      }
    }
    function populateCityBox(){
     if (req.readyState == 4) { // Complete
      if (req.status == 200) { // OK response
        }
      }else {  
            //alert("Problem with server response:" + req.statusText);
        }
    }
}

function callHome()
{
  document.headerForm.action = "HomeAction.do";
  document.headerForm.methodToCall.value="loadData";
  document.headerForm.submit();
} 
     </script>
