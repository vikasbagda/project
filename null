<script>

var MenuUL = document.getElementById("menuul");
var topMenuLen = aSubmenuData.length;
var labels =[];

for(var i=0; i< topMenuLen; i++){              
                        var label = aSubmenuData[i].id;
                         var templabels = label.split('^');
                         labels[labels.length] =  templabels[1];     
}

for(var i=0; i< topMenuLen; i++){              
            MenuUL.innerHTML += "<li class='yuimenubaritem first-of-type'><a class='yuimenubaritemlabel href='#" + aSubmenuData[i].id +"'> " +labels[i]+"</a></li>";
}

</script>


<script type="text/javascript" language="javascript">  
 
//Added by Priyanka for time out session
           var timeout = '1793000';    //30000
                var t = ''; 
           var timer = setInterval(
           function() 
           { 
            timeout -= 1000; 
            window.status = time(timeout);  
            if (timeout <= 0) 
            { 
            clearInterval(timer); 
            window.navigate("logout.jsp");
            alert('Session expired.Please login again.'); 
            }
            if (timeout == '300000')  //10000
            { 
       //     clearInterval(timer); 
        //    window.navigate("logout.jsp")
            alert('Your Session will expire in 5 min.'); 

          //  return "Your session will timeout in " + t + " minutes.";   

            } 
            }, 1000);     
           function two(x) { return ((x > 9) ? "" : "0") + x }     
           function time(ms) {   
  
               var sec = Math.floor(ms / 1000);   
               ms = ms % 1000    
               var min = Math.floor(sec / 60);   
               sec = sec % 60;   
               t = two(sec);     
               var hr = Math.floor(min / 60);   
               min = min % 60;   
               t = two(min) + ":" + t;   
                  
               return "Your session will timeout in " + t + " minutes.";   
           }   
 
//Ended by Priyanka for time out session
</script>

