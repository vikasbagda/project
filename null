$(window).load(function()
  {    
 	var ht = parseInt ($(".gridstyle div").height());
 var fullwidth = 100;
	var halfwidth = 98;
   if( ht > 200)
	  {
	    $(".gridstyle div").height(200);
      $(".gridstyle div").css("overflow-y", "auto", "overflow-x", "hidden");
      $(".gridstyle table").css("width", halfwidth+"%");
      $(".gridstyle div table").css("width", halfwidth+"%");
     /* $(".gridstyle table").width(740);
      $(".gridstyle div table").width(740);*/
	  }
    else
    {
    /* $(".gridstyle table").width(750);
     $(".gridstyle div table").width(750);*/
     $(".gridstyle table").css("width", fullwidth+"%");
     $(".gridstyle div table").css("width", fullwidth+"%");
   
    }
    
 	var divht = parseInt ($(".headerfix").height());
	var divwt = parseInt ($(".headerfix table").width());
	var halfwidth = 98;
  var headheight = divht+24;
  if( divht > 250)
	{ 	
	  $(".headerfix").height(250);
	  $(".headerfix").css("overflow-y", "auto");	 
	}
	else
	{
		 $(".headerfix").height(headheight);
     $(".headerfix").css("overflow-y", "hidden");	
	}
	 if(divwt >= 970)
	  {   
      
       $(".headerfix").width(970);
      $(".headerfix table").css("width", halfwidth+"%");
      $(".headerfix #long").width(divwt);	 
      $(".headerfix").css("overflow", "auto");	 
     }
	  else
	  {
      $(".headerfix table").css("width", halfwidth+"%");
	  }
  
});
