    <script type="text/javascript" >
var browserUrl = document.URL;
//alert(browserUrl);

  function validateLoginDetails()
  {
    var userId = document.logindetailsForm.userId.value;
    var password = document.logindetailsForm.password.value;
    var splCharTest=/[\W_]/g;
    var upperCaseTest= /[A-Z]/;
    var lowerCaseTest= /[a-z]/;
    var numberTest= /[0-9]/;
    
    if(userId==null || userId=="")
    {
        alert("Please enter User name.");
        document.logindetailsForm.userId.value=""
        document.logindetailsForm.userId.focus();
        return false;
    }
    else if(password==null || password=="")
    {
        alert("Please enter Password.");
        document.logindetailsForm.password.value=""
        document.logindetailsForm.password.focus();
        return false;
    }
      document.logindetailsForm.brsrUrl.value=browserUrl;
//      alert(document.logindetailsForm.brsrUrl.value);
      document.logindetailsForm.submit();
  }
  
  function doOnload()
    {
      document.logindetailsForm.userId.focus();        
    }
   
    </script>
